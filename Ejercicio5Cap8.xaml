<Window x:Class="Cap8_Cap9.Ejercicio5Cap8"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cap8_Cap9"
        mc:Ignorable="d"
        Title="Ejercicio5Cap8" Height="350" Width="400">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="5">
            <StackPanel>
                <Label Content="Cadena 1"/>
                <StackPanel>
                    <TextBox x:Name="Cadena1TextBox" Height="25"/>
                    <StackPanel>
                        <Label Content="Cadena2"/>
                        <StackPanel>
                            <TextBox x:Name="Cadena2TextBox" Height="25"/>
                            <Button x:Name="OrdenarCadenas" Content="Ver en orden alfabetico" Height="25" Width="130" Margin="0,15" Click="OrdenarCadenas_Click"/>
                        </StackPanel>
                        <ListBox x:Name="OrdenarListBox" Height="40" />
                    </StackPanel>
                </StackPanel>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
